{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "vite-env.d.ts",
    "../../helpers/githubHelp/DashnoardOrders.vue",
    "../../helpers/githubHelp/DashboardCategories.vue",
    "../../helpers/githubHelp/SharedCounterCard.vue"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "types": ["element-plus/global", "vite/client"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "strict": true
  }
}
